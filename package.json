    "@fastify/cors": "^10.0.1",
    "@fastify/multipart": "^9.0.1",
    "@fastify/session": "^11.0.1",
    "@fastify/static": "^8.0.2",
    "axios": "^1.6.5",
    "bcrypt": "^6.0.0",
    "better-sqlite3": "^9.2.2",
    "dotenv": "^16.3.1",
    "fastify": "^5.2.0",
    "pino-pretty": "^11.0.0"
  }
